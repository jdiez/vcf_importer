{
  "name": "vcf_to_simplevar",
  "title": "Import VCF",
  "summary": "Converts .vcf file to a DNAnexus table",
  "description": "Converts .vcf file to a DNAnexus table for subsequent processing and visualization in the DNAnexus platform",
  "categories": ["Importers"],
  "dxapi": "1.03rc",
  "inputSpec": [
    {"name": "vcf", "class": "file"},
    {"name": "reference", "class": "record", "type": "ContigSet"},
    {"name": "output name", "class":"string", "optional":true},
    {"name": "compress_reference", "class": "boolean", "default": false},
    {"name": "infer_no_call", "class": "boolean", "default": false},
    {"name": "store_full_vcf", "class": "boolean", "default": true},
    {"name": "store_samples_individually", "class":"boolean", "default":false}
  ],
  "outputSpec": [
    {"name": "variants", "class": "array:gtable", "type": {"$and": ["SimpleVar", "gri"]}}
  ],
  "runSpec": {"file": "parseVcf.py", "interpreter": "python2.7_old"}
}
