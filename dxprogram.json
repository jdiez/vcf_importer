{
  "name": "vcf_to_simplevar",
  "title": "VCF Importer (SimpleVar)",
  "summary": "Convert .vcf file to SimpleVar GTable",
  "categories": ["Importer"],
  "dxapi": "1.03rc",
  "version": "0.1",
  "inputs": [
    {"name": "vcf", "class": "file"},
    {"name": "reference", "class": "record", "type": "ContigSet"},
    {"name": "compress_reference", "class": "boolean", "default": true},
    {"name": "compress_no_call", "class": "boolean", "default": true},
    {"name": "store_full_vcf", "class": "boolean", "default": true},
    {"name": "store_samples_individually", "class":"boolean", "default":false}
  ],
  "outputs": [
    {"name": "simplevar", "class": "array:gtable"}
  ],
  "run": {"file": "parseVcf.py", "interpreter": "python2.7"}
}
